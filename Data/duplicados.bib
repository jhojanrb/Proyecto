@article{ref_dup1538,
  title = {Fostering computational thinking in engineering education: Challenges, examples, and best practices}},
  author = {Gross S.; Kim M.; Schlosser J.; Mohtadi C.; Lluch D.; Schneider D.}},
  year = {2014}},
  journal = {IEEE Global Engineering Education Conference, EDUCON}},
  abstract = {Computational Thinking is a core capability for most engineers. The term summarizes a set of skills needed to transform real-life challenges into problems that can be solved with the help of a computer and to apply computer-based solutions to questions at hand. This mindset is fundamental to almost every engineering task. © 2014 IEEE.}},
