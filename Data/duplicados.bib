@article{ref_dup1392,
  title = {Using robot to motivate computational thinking in high school students}},
  author = {Enriquez C.; Aguilar O.; Domínguez F.}},
  year = {2016}},
  journal = {IEEE Latin America Transactions}},
  abstract = {The importance today is the development of computational thinking of technological professionals. This kind of thinking is useful for solving algorithms, analyzing, designing, coding and generating tests in a computer language. Today, obtaining knowledge and skills is related to the use of technology. In this paper, a robot is used as a teaching element to motivate the generation process of computational thinking in students of high school level. Measurement of variables, such as: Advantages, interest, competition and operating time serve as indices to determine the degree of acceptance of this educational tool. © 2003-2012 IEEE.}},
