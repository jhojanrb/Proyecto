@article{ref_dup1340,
  title = {An approach based on Embodied Programming to teach computer science at a Secondary School}},
  author = {Cornejo M.E.; Sommer S.; Rodriguez J.}},
  year = {2017}},
  journal = {Proceedings - International Conference of the Chilean Computer Science Society, SCCC}},
  abstract = {In this article a didactic approach is introduced which seeks to integrate the progress made in the context of Block-Based Programming and other didactic approaches developed in the area of computer teaching. The prospects developed in the area of Project Based Learning Approach, Collaborative Learning and Embodied Programming are taken into account. Surrogate Embodiment, in the Embodied Programming context, is a kind of interaction where the movements performed by a person on a stage are directed by students. Furthermore, it is used as a structuring resource in the teaching process, the development of computational devices, paying special attention to skills development in the area of Computational Thinking. This paper presents a Video Game Development Workshop based on this approach with first year students from a state technical school. The experience has shown satisfactory results in connection to the acquisition of concepts and fundamental practices in the area of Algorithms and Programming. © 2017 IEEE.}},
