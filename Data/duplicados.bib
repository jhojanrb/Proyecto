@article{ref_dup850,
  title = {Building Conceptual Models and Engineering Design Thinking in K12 Students}},
  author = {Ruzycki N.J.; Imperial L.D.F.; Dulany K.; Carter J.; Kerr S.N.; Danger C.A.; Arnold L.}},
  year = {2021}},
  journal = {Proceedings of 2021 World Engineering Education Forum/Global Engineering Deans Council, WEEF/GEDC 2021}},
