@article{ref_dup727,
  title = {SzHNN: A Novel and Scalable Deep Convolution Hybrid Neural Network Framework for Schizophrenia Detection Using Multichannel EEG}},
  author = {Sharma G.; Joshi A.M.}},
  year = {2022}},
  journal = {IEEE Transactions on Instrumentation and Measurement}},
  abstract = {In the field of neuroscience, brain activity measurement and analysis are considered crucial areas. Schizophrenia (Sz) is a brain disorder that severely affects the thinking, behavior, and feelings of people worldwide. Thus, an accurate and rapid detection method is needed for proper care and quality treatment of the patients. Electroencephalography (EEG) is proved to be an efficient biomarker in Sz detection as it records brain activities. This article aims to improve the performance of EEG-based Sz detection using a deep-learning approach in remote applications. A hybrid deep-learning model identified as schizophrenia hybrid neural network (SzHNN), which is a combination of convolutional neural networks (CNNs) and long short-term memory (LSTM), has been proposed wherein the CNN for local feature extraction and LSTM for classification is utilized. In this article, the proposed model has been compared with several deep-learning and machine-learning-based models. All the models have been evaluated on two different datasets wherein dataset 1 consists of 19 subjects and dataset 2 consists of 16 subjects. The proposed model is also implemented with the Internet-of-Medical-Things (IoMT) framework for smart healthcare and remote-based applications. Several experiments have been conducted using various parametric settings on different frequency bands and different sets of electrodes on the scalp. Based on all the experiments, it is evident that the proposed hybrid model (SzHNN) provides the highest classification accuracy of 99.9% compared to other implemented models and existing models of previous papers. The proposed model overcomes the influence of different frequency bands and shows a better accuracy of 96.10% (dataset 1) and 91.00% (dataset 2) with only five electrodes. Subject-wise testing is also done for SzHNN, which proposes an accuracy of 90.11% and 89.60% for datasets 1 and 2, respectively.  © 1963-2012 IEEE.}},
  url = {https://www.scopus.com/record/display.uri?eid=2-s2.0-85139828617},
  shared_files = {Data/resultados_ieee.bib, Data/resultados_Scopus.bib}
}

@article{ref_dup728,
  title = {A Model of Neural Mechanisms Underlying Functional Systems of an Aeronavigation System Operator}},
  author = {Kryzhanovsky G.A.; Kostin G.A.; Shestakov I.N.; Samoilov V.A.}},
  year = {2022}},
  journal = {2022 19th Technical Scientific Conference on Aviation Dedicated to the Memory of N.E. Zhukovsky, TSCZh 2022}},
  abstract = {This study seeks to develop a model that would facilitate professional skill development in aeronavigation system operators by considering the neurophysiology that underlies the formation and development of functional systems. A key role is played by motivation which is realized by electrochemical processes in synapses. We present a model of synapse formation in the process of learning. A number of studies using the Brainbit brain-computer were carried out as a part of various activities, including the performance of tasks with the Expert air traffic controller simulator. Findings from computational processing of electroencephalography signals revealed the degree of synchronization of brain hemispheres depending on the complexity of the tasks. We propose to use a ratio of mean values of direct and inverse correlation across short epochs as a synchronization (coordination) factor for the activities of both hemispheres. We were able to identify regularities behind the change of the coordination factor depending on the complexity of the task and the degree of involvement in its performance. Three levels of task complexity (high, medium and low) were identified. The coordination factor will vary depending on the complexity level of a particular task. We were able to determine that a high involvement in the performance of a medium-complexity task is accompanied by a high level of coordination. This lets us determine the attained development level of professional and mental capabilities of an aeronavigation system operator and to develop an individual training path.  © 2022 IEEE.}},
  url = {https://www.scopus.com/record/display.uri?eid=2-s2.0-85134157438},
  shared_files = {Data/resultados_ieee.bib, Data/resultados_Scopus.bib}
}

@article{ref_dup729,
  title = {A method for assessing computational thinking in students using source code analysis}},
  author = {Pacheco-Portuguez S.; Gonzalez-Torres A.; Sancho-Chavarria L.; Trejos-Zelaya I.; Monge-Fallas J.; Navas-Su J.; Canas A.J.; Rodriguez A.; Chinchilla C.A.}},
  year = {2022}},
  journal = {Proceedings - 2022 International Conference on Advanced Learning Technologies, ICALT 2022}},
  abstract = {This paper summarizes research in progress, at a national scale, that analyzes large volumes of elementary school and high school projects to assess the development of skills, attitudes, and practices that students develop when solving problems via computer programming, grounded on their under-standing of fundamental concepts in computing. The approach is independent of programming languages and uses a generic abstract syntax tree and projects' metadata to calculate metrics, and establish relationships between measures, school regions, educational centers, and student groups. The research relates source code analysis using abstract syntax trees to some of the main computational thinking concepts. Preliminary results obtained using the proposed method are presented and discussed.  © 2022 IEEE.}},
  url = {https://www.scopus.com/record/display.uri?eid=2-s2.0-85136997402},
