}

@article{ref_dup1499,
  title = {Evaluating the performance of Group Counseling Optimizer on CEC 2014 problems for Computational Expensive Optimization}},
  author = {Biswas S.; Eita M.A.; Das S.; Vasilakos A.V.}},
