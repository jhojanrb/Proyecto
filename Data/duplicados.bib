@article{ref_dup553,
  title = {Design and Effectiveness Analysis of Interactive Strategies for Teaching Graphical Programming Oriented to Cultivate Computational Thinking}},
  author = {Wang F.; Chen L.; Wang W.}},
  year = {2023}},
